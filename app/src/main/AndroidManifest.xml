<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.univdhaka.cse.cse2216.myshop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyShop">
        <activity
            android:name=".CartDetailsActivity"
            android:label="Cart Details"
            android:parentActivityName=".HistoryActivity" />
        <activity
            android:name=".HistoryActivity" android:parentActivityName=".HomeActivity"
            android:label="History">


        </activity>
        <activity
            android:name=".AddProductActivity"
            android:label="ADD PRODUCT"
            android:parentActivityName=".ProductActivity" />
        <activity
            android:name=".ProductActivity" android:parentActivityName=".HomeActivity"
            android:label="Products">


        </activity>
        <activity
            android:name=".HomeActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>